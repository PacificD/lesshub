{
  "name": "@pacificd/hooks",
  "version": "0.1.5",
  "author": "PacificD",
  "description": "React custom hooks",
  "keywords": [
    "react",
    "react hooks"
  ],
  "license": "ISC",
  "repository": "https://github.com/pacificd/lesshub",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
