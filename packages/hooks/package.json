      "import": "./dist/useSnowflakes.mjs",
      "types": "./dist/useSnowflakes.d.ts"
    }
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf node_modules"
  },
  "devDependencies": {
    "react": "latest",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
